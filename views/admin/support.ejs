		<li style="float:right"><a href="/admin/student_list">Student List</a></li>
	</ul>
	<center>
	<table border="1" width="400px" >
		<tr>
			<th>ID</th>
			<th>Text</th>
			<th>Time</th>
			<th>Seen</th>
		</tr>

		<% for(var i=0; i < uList.length; i++){ %>
		<tr>
			<td><a href="/admin/teacher_list/<%= uList[i].T_ID %>">	<%= uList[i].T_ID %></a></td>
			<td><%= uList[i].S_TEXT %></td>
			<td><%= uList[i].S_TIME %></td>
			<td><a href="/admin/support/seen/<%= uList[i].T_ID %>">Check</a></td>
			<!-- <td><%= uList[i].S_STATUS %></td> -->
		
		</tr>
		<% } %>
	</table>
</center>
</body>
